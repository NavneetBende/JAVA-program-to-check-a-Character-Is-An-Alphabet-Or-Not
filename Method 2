//C Program to find character is alphabet or not

#include <stdio.h>
int main()
{
   char ch='k';

      
 
  if( (ch>=97 && ch<=122) || (ch>=65 && ch<=90))
     printf("The inserted character %c is an Alphabet", ch);
   
  else
    printf("The entered character %c is not an Alphabet", ch);
  return 0;
}
